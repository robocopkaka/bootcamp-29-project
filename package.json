{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "babel-node": "babel-node --presets=latest",
    "start": "nodemon --exec npm run babel-node ./server/bin/www",
    "start:react": "webpack-dev-server --config webpack.dev.js",
    "start:dev": "nodemon --exec npm run babel-node ./server/bin/www",
    "start:all": "nf start -p 8000",
    "build": "babel lib -d dist --presets=latest",
    "react:build": "webpack --config webpack.prod.js",
    "serve": "node dist/index.js",
    "mocha": "mocha --compilers js:babel-core/register",
    "db:migrations": "sequelize db:migrate:undo:all && sequelize db:migrate && sequelize db:seed:all",
    "pretest": "NODE_ENV=test npm run db:migrations",
    "test:jest": "jest --coverage",
    "test": "nyc --reporter=html --reporter=text npm run test:express && npm run test:jest",
    "test:react": "NODE_ENV=test nyc --reporter=html --reporter=text ./node_modules/karma/bin/karma start karma.conf.js",
    "test:express": "NODE_ENV=test mocha --compilers js:babel-core/register --require babel-polyfill --recursive ./server/test/ --timeout 10000 --exit",
    "coverage": "nyc report --reporter=text-lcov | coveralls",
    "coveralls": "cat ./coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
